<?xml version="1.0" encoding="utf-8"?>
<!--圆角为4dp的的蓝色背景-->
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!--背景色-->
    <solid android:color="@android:color/white" />
    <!--边框线-->
    <stroke
        android:width="1dp"
        android:color="@color/darkgrey" />
    <!--圆角-->
    <corners
        android:radius="32dp"/>
</shape>